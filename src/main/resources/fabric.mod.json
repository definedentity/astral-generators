{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "definedentity"
  ],
  "contact": {
    "homepage": "https://github.com/definedentity/astral-generators",
    "sources": "https://github.com/definedentity/astral-generators"
  },
  "license": "${mod_license}",
  "icon": "assets/astralgenerators/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "value": "dev.definedentity.astralgenerators.AstralGenerators",
        "adapter": "kotlin"
      }
    ],
    "client": [
      {
        "value": "dev.definedentity.astralgenerators.AGClient",
        "adapter": "kotlin"
      }
    ],
    "fabric-datagen": [
      {
        "value": "dev.definedentity.astralgenerators.AGDataGen",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "astralgenerators.mixins.json"
  ],
  "accessWidener": "astralgenerators.accesswidener",
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.18.2",
    "java": ">=17",
    "fabric-api": "${fabric_version}",
    "fabric-language-kotlin": "*",
    "xenon": "*",
    "botarium": "2.0.5"
  },
  "suggests": {
    "ctm-refabricated": "*"
  }
}
